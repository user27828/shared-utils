{
  "name": "@user27828/shared-utils",
  "description": "Collection of common utilities I use for various projects",
  "version": "0.69.7",
  "type": "module",
  "packageManager": "yarn@4.12.0",
  "engines": {
    "node": ">=18"
  },
  "workspaces": [
    "client",
    "utils",
    "server",
    "test-consumer",
    "test-consumer/react-app",
    "test-consumer/server",
    "test-consumer/node-consumer",
    "test-consumer/vanilla-js-consumer"
  ],
  "author": {
    "name": "Marc Stephenson",
    "url": "https://github.com/user27828"
  },
  "license": "MIT",
  "main": "dist/utils/index.js",
  "types": "dist/utils/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.js",
      "default": "./dist/utils/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.js",
      "default": "./dist/client/index.js"
    },
    "./client/wysiwyg": {
      "types": "./dist/client/wysiwyg.d.ts",
      "import": "./dist/client/wysiwyg.js",
      "require": "./dist/client/wysiwyg.js",
      "default": "./dist/client/wysiwyg.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.js",
      "default": "./dist/utils/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    },
    "./cms": {
      "types": "./dist/utils/src/cms/index.d.ts",
      "import": "./dist/utils/src/cms/index.js",
      "require": "./dist/utils/src/cms/index.js",
      "default": "./dist/utils/src/cms/index.js"
    },
    "./cms/server": {
      "types": "./dist/server/src/cms/index.d.ts",
      "import": "./dist/server/src/cms/index.js",
      "require": "./dist/server/src/cms/index.js",
      "default": "./dist/server/src/cms/index.js"
    },
    "./cms/client": {
      "types": "./dist/client/src/cms/index.d.ts",
      "import": "./dist/client/src/cms/index.js",
      "require": "./dist/client/src/cms/index.js",
      "default": "./dist/client/src/cms/index.js"
    },
    "./fm": {
      "types": "./dist/utils/src/fm/index.d.ts",
      "import": "./dist/utils/src/fm/index.js",
      "require": "./dist/utils/src/fm/index.js",
      "default": "./dist/utils/src/fm/index.js"
    },
    "./fm/server": {
      "types": "./dist/server/src/fm/index.d.ts",
      "import": "./dist/server/src/fm/index.js",
      "require": "./dist/server/src/fm/index.js",
      "default": "./dist/server/src/fm/index.js"
    },
    "./fm/client": {
      "types": "./dist/client/src/fm/index.d.ts",
      "import": "./dist/client/src/fm/index.js",
      "require": "./dist/client/src/fm/index.js",
      "default": "./dist/client/src/fm/index.js"
    },
    "./fm/server/s3": {
      "types": "./dist/server/src/fm/storage/FmStorageS3.d.ts",
      "import": "./dist/server/src/fm/storage/FmStorageS3.js",
      "require": "./dist/server/src/fm/storage/FmStorageS3.js",
      "default": "./dist/server/src/fm/storage/FmStorageS3.js"
    },
    "./utils/*": "./dist/utils/*"
  },
  "typesVersions": {
    "*": {
      "client": [
        "dist/client/index.d.ts"
      ],
      "client/wysiwyg": [
        "dist/client/wysiwyg.d.ts"
      ],
      "utils": [
        "dist/utils/index.d.ts"
      ],
      "server": [
        "dist/server/index.d.ts"
      ],
      "cms": [
        "dist/utils/src/cms/index.d.ts"
      ],
      "cms/server": [
        "dist/server/src/cms/index.d.ts"
      ],
      "cms/client": [
        "dist/client/src/cms/index.d.ts"
      ],
      "fm": [
        "dist/utils/src/fm/index.d.ts"
      ],
      "fm/server": [
        "dist/server/src/fm/index.d.ts"
      ],
      "fm/client": [
        "dist/client/src/fm/index.d.ts"
      ],
      "fm/server/s3": [
        "dist/server/src/fm/storage/FmStorageS3.d.ts"
      ]
    }
  },
  "scripts": {
    "client": "yarn workspace @shared-utils/client",
    "utils": "yarn workspace @shared-utils/utils",
    "server": "yarn workspace @shared-utils/server",
    "build": "yarn utils build && yarn client build && yarn server build",
    "prepare": "yarn build 2>/dev/null || echo '⚠️ Build failed during prepare, continuing...'",
    "postinstall": "timeout 50s yarn build 2>/dev/null || echo '⚠️ Build failed during postinstall, continuing. This is a graceful failure.'",
    "clean": "rm -fr ./node_modules/{*,.*} .yarn/cache || true",
    "test": "cd client && yarn test && cd ../utils && yarn test && cd ../server && yarn test && cd ..",
    "test:consumer": "[ -d test-consumer ] && cd test-consumer && yarn dev || echo '❌ test-consumer directory not found - this script is only available in the main shared-utils repository'",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "upgrade": "npx yarn-upgrade-interactive",
    "update:data": "./bin/update-source-data.sh",
    "compare:data": "./bin/compare-data.cjs"
  },
  "bin": {
    "cf:setup-turnstile-worker": "./scripts/setup-turnstile-worker.sh",
    "dependency-manager": "./scripts/dependency-manager.js",
    "docker-build": "./bin/docker-build.sh",
    "killnode": "./bin/killnode.sh",
    "vite-preview-envs": "./bin/vite-preview-envs.sh",
    "yarn-upgrade-interactive": "./bin/package-upgrades.sh"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@types/jest": "^30.0.0",
    "@types/nanoid": "^3.0.0",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "jest": "^30.2.0",
    "jest-environment-node": "^30.2.0",
    "jsdom": "^28.1.0",
    "prettier": "^3.8.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@tinymce/tinymce-react": "^6.3.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dompurify": "^3.3.1",
    "dotenv": "^17.3.1",
    "dotenv-expand": "^12.0.3",
    "lodash-es": "^4.17.23",
    "marked": "^17.0.3",
    "nanoid": "^5.1.6",
    "papaparse": "^5.5.3",
    "tinymce": "^8.3.2"
  },
  "peerDependencies": {
    "@aws-sdk/client-s3": ">=3.0.0",
    "@aws-sdk/s3-request-presigner": ">=3.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-s3": {
      "optional": true
    },
    "@aws-sdk/s3-request-presigner": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "files": [
    "dist/**/*",
    "package.json",
    "README.md",
    "LICENSE",
    "scripts/**/*",
    "bin/**/*"
  ]
}
