{
  "name": "@shared-utils/client",
  "version": "0.69.3",
  "description": "React components and client-side utilities",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./wysiwyg": {
      "types": "./dist/wysiwyg.d.ts",
      "import": "./dist/wysiwyg.js",
      "default": "./dist/wysiwyg.js"
    },
    "./*": "./dist/*"
  },
  "scripts": {
    "build": "rm -f tsconfig.tsbuildinfo && (yarn --version > /dev/null 2>&1 && yarn tsc --build || npx tsc --build)",
    "build:clean": "rm -f tsconfig.tsbuildinfo && rm -rf ../dist/client && yarn build",
    "test": "vitest run",
    "test:watch": "vitest",
    "dev": "yarn tsc --outDir ../dist/client --watch"
  },
  "peerDependencies": {
    "@mui/icons-material": ">=5.0.0",
    "@mui/material": ">=5.0.0",
    "@tinymce/tinymce-react": ">=5.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "tinymce": ">=6.0.0"
  },
  "peerDependenciesMeta": {
    "@tinymce/tinymce-react": {
      "optional": true
    },
    "tinymce": {
      "optional": true
    }
  },
  "dependencies": {
    "@shared-utils/utils": "workspace:*",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dompurify": "^3.2.7",
    "papaparse": "^5.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.2.0",
    "@types/jest": "^30.0.0",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.9",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^3.2.4",
    "babel-jest": "^30.1.2",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "resolutions": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1"
  },
  "files": [
    "dist"
  ]
}
